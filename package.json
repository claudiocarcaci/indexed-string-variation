{
  "name": "indexed-string-variation",
  "version": "0.0.1",
  "description": "Experimental JavaScript module to generate all possible variations of strings over an alphabet using an n-ary virtual tree",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib/index.js",
    "src/index.js"
  ],
  "scripts": {
    "build": "node_modules/.bin/babel --presets=es2015 src/ -d lib",
    "test": "node_modules/.bin/tap lib/test.js --coverage | node_modules/.bin/tap-spec",
    "posttest": "node_modules/.bin/tap --coverage-report=lcov && node_modules/.bin/codecov --file 'lib/index.js'",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Luciano Mammino",
    "email": "lucianomammino@gmail.com",
    "url": "http://loige.co"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lmammino/indexed-string-variation"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "codecov": "^1.0.1",
    "tap": "^7.0.0",
    "tap-spec": "^4.1.1"
  }
}
